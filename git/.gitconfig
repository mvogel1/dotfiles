[user]
	name = Michael Vogel
	username = mvogel1
	email = mvogel1@web.de

[alias]
	a = add
	b = branch
	c = commit
	s = status
	sw = switch
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	lg = log --oneline --graph
	lgo = log --oneline
	unstage = reset HEAD --
	last = log -1 HEAD
[init]
	defaultBranch = main